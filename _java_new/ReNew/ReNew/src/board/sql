board TABLE 생성
bno INT AI : 번호
title VARCHAR: 제목 
writer VARCHAR : 작성자
content : 내용
regdate : 작성일
moddate : 수정일

CREATE TABLE board(
bno INT AUTO_INCREMENT,
title VARCHAR(200) NOT NULL,
writer VARCHAR(200) NOT NULL,
content TEXT,
regdate DATETIME DEFAULT NOW(),
moddate DATETIME DEFAULT NOW(),
PRIMARY KEY(bno));